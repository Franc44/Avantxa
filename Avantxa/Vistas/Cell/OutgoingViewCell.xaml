<?xml version="1.0" encoding="UTF-8"?>
<ViewCell xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="Avantxa.Vistas.Cell.OutgoingViewCell">
    <Frame BackgroundColor="#DDE1E4"
            CornerRadius="5"
            Padding="10"
            Rotation="180"
            FlowDirection="LeftToRight"
            HasShadow="false"
            Margin="80,10,10,10">
           <Label Text="{Binding Text}"
                  TextColor="Black"
                  FontFamily="Abo" FontSize="18"/>
    </Frame>
</ViewCell>
